KafkaServer {
   org.apache.kafka.common.security.plain.PlainLoginModule required
   username="{{ kafka_user }}"
   password="{{ kafka_pass }}"
   user_{{ kafka_user }}="{{ kafka_pass }}";
};

Client {
   org.apache.zookeeper.server.auth.DigestLoginModule required
   username="{{ zookeeper_user }}"
   password="{{ zookeeper_pass }}";
};

